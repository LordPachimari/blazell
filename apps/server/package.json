{
	"name": "@pachi/server",
	"version": "1.0.0",
	"main": "src/index.ts",
	"scripts": {
		"beautify": "biome format ./src --write",
		"dev": "wrangler dev src/index.ts",
		"deploy-prod": "wrangler deploy --minify --prod src/index.ts",
		"deploy-dev": "wrangler deploy --minify --env dev src/index.ts",
		"test": "vitest run",
		"lint": "biome lint src",
		"format": "biome format ./src",
		"local-generate": "wrangler d1 execute pachi-db-dev  --local --file=../../packages/db/migrations/0000_chilly_warlock.sql",
		"login": "wrangler login",
		"typecheck": "tsc --noEmit",
		"create:KV": "wrangler kv:namespace create KV"
	},
	"dependencies": {
		"@clerk/backend": "^0.38.6",
		"@hono/clerk-auth": "^1.0.1",
		"@pachi/api": "^0.1.0",
		"@pachi/core": "^0.1.0",
		"@pachi/db": "^0.1.0",
		"@pachi/utils": "^0.1.0",
		"@pachi/validators": "^0.1.0",
		"drizzle-orm": "^0.30.7",
		"effect": "^3.1.5",
		"hono": "^4.2.8",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.1",
		"@cloudflare/workers-types": "^4.20240405.0",
		"@pachi/tsconfig": "workspace:*",
		"miniflare": "^3.20230922.0",
		"typescript": "^5.4.5",
		"vitest": "^0.34.6",
		"vitest-environment-miniflare": "^2.14.1",
		"wrangler": "^3.51.2"
	}
}
